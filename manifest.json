{
	"name": "Magnetify",
	"version": "1.0.0",
	"manifest_version": 2,
	
	"description": "This extension will make all spotify links open in your desktop app instead of the web player",
	"icons": { 	"16": "img/icon16.png",
				"48": "img/icon48.png",
				"128": "img/icon128.png" },
				
	"browser_action": {
		"default_icon": "img/icon48.png",
		"default_popup": "src/popup/popup.html"
	},
	
	"author": "Lasse Brudeskar Vik√•s",
	"background": {
		"page" : "src/background/background.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"permissions": [
		"tabs", "storage", "<all_urls>", "contextMenus"
	],
	"options_page": "src/options/options.html",
	"web_accessible_resources": [
		"lib/jquery-2.0.3.min.js", "lib/jquery-2.0.3.min.map"
	]

}