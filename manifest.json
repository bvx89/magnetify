{
	"name": "Magnetify",
	"version": "0.9.4",
	"manifest_version": 2,
	
	"description": "This extension will make all spotify links open in your desktop app instead of the web player",
	"icons": { 	"16": "img/icon16.png",
				"48": "img/icon48.png",
				"128": "img/icon128.png" },
				
	"browser_action": {
		"default_icon": "img/icon48.png",
		"default_popup": "src/popup/popup.html"
	},
	
	"author": "Lasse Brudeskar Vik√•s",
	"background": {
		"scripts": ["lib/jquery-2.0.3.min.js", "lib/storage.js", "src/background.js"],
		"persistent": false
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"permissions": [
		"tabs", "storage", "<all_urls>", "http://www.spoti.fi/", "http://ws.spotify.com/lookup"
	],
